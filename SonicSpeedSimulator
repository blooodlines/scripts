
loadstring(game:HttpGet("https://raw.githubusercontent.com/blooodlines/libs/main/elerium"))()

local qq = library:AddWindow("Sonic Speed Simulator")
local HomeTab = qq:AddTab("Home")
local MainTab = qq:AddTab("Main")
local MiscTab = qq:AddTab("Miscellaneous")
local SettingsTab = qq:AddTab("Settings")

local LocalPlayer = game:GetService("Players").LocalPlayer

local WorldCurrencies = workspace:FindFirstChild("World Currencies")
local Objects = workspace.Map:FindFirstChild("Objects")
local Destructibles = Objects:FindFirstChild("Destructibles")

local Knit = game:GetService("ReplicatedStorage").Knit

-- Services
local Services = Knit.Services
local ZoneService = Services.ZoneService
local WorldCurrencyService = Services.WorldCurrencyService
local DestructibleService = Services.DestructibleService
local SpinWheelService = Services.SpinWheelService

-- Remotes
local PickupCurrency = WorldCurrencyService.RE.PickupCurrency
local ProcessDestructible = DestructibleService.RE.ProcessDestructible

local RequestFreeSpin = SpinWheelService.RF.RequestFreeSpin
local RequestTeleportToZone = ZoneService.RF.RequestTeleportToZone
local CompleteZoneObby = ZoneService.RF.CompleteZoneObby
local RequestUnlockCharacter = Services.CharacterService.RE.RequestUnlockCharacter

for _,v in pairs(getconnections(LocalPlayer.Idled)) do
     v:Disable()
end

HomeTab:AddLabel("Scripted by infinite#7302")
HomeTab:AddLabel("UI Library by @Singularity")

HomeTab:AddButton("Copy Discord Server Link", function()
	setclipboard("https://discord.gg/ssaVPA6atd")
end)

MainTab:AddLabel("Main")

MainTab:AddSwitch("Auto Pop Balloons & Crates", function(a)
getgenv().AutoBreak = a
while AutoBreak do task.wait(0.3)
for _,v in pairs(Destructibles:GetChildren()) do
     if v:FindFirstChild("base").Attachment:FindFirstChild("dus") and v then
	          ProcessDestructible:FireServer(v.Name)
	      end
       end
    end
end)

MainTab:AddSwitch("Auto Collect Everything", function(b)
getgenv().AutoCollect = b
while AutoCollect do task.wait(5)
for _,v in pairs(WorldCurrencies:GetChildren()) do
     if v:IsA("Model") and v then
              PickupCurrency:FireServer(v.Name) v:Destroy()
	      end
       end
    end
end)

MainTab:AddSwitch("Auto Rebirth", function(c)
getgenv().AutoRebirth = c
while AutoRebirth do task.wait(3)
         Services.LevelingService.RF.AttemptRebirth:InvokeServer()
    end
end)

MiscTab:AddLabel("Miscellaneous")

MiscTab:AddSwitch("Auto Complete Obbys", function(d)
getgenv().AutoComplete = d
while AutoComplete do task.wait(.5)
       RequestTeleportToZone:InvokeServer("Green Hill Obby 1") task.wait(0.2)
       ZoneService.RE.ZoneLoaded:FireServer("Green Hill Obby 1") task.wait(0.2)
       ZoneService.RF.FinishObby:InvokeServer("Green Hill Obby 1", true)
       RequestFreeSpin:InvokeServer("GreenHill1")
	   task.wait(0.2)

	   RequestTeleportToZone:InvokeServer("Green Hill Obby 2") task.wait(0.2)
       ZoneService.RE.ZoneLoaded:FireServer("Green Hill Obby 2") task.wait(0.2)
       ZoneService.RF.FinishObby:InvokeServer("Green Hill Obby 2", true) task.wait(0.2)
       RequestFreeSpin:InvokeServer("GreenHill2")
	   task.wait(0.2)

	   RequestTeleportToZone:InvokeServer("Green Hill Obby 3") task.wait(0.2)
       ZoneService.RE.ZoneLoaded:FireServer("Green Hill Obby 3")
       ZoneService.RF.FinishObby:InvokeServer("Green Hill Obby 3", true) task.wait(0.2)
       RequestFreeSpin:InvokeServer("GreenHill3")
	   task.wait(0.1)
    end
end)

MiscTab:AddButton("Unlock all Worlds", function()
    RequestTeleportToZone:InvokeServer("Lost Valley Obby", "Green Hill Exit")
    CompleteZoneObby:InvokeServer()
    task.wait(0.3)
    
    RequestTeleportToZone:InvokeServer("Emerald Hill Obby", "Lost Valley Exit")
    CompleteZoneObby:InvokeServer()
    task.wait(0.3)
    
    RequestTeleportToZone:InvokeServer("Snow Valley Obby", "Emerald Hill Exit")
    CompleteZoneObby:InvokeServer()

    task.wait(0.3)
    
    RequestTeleportToZone:InvokeServer("Hill Top Zone Obby", "Snow Valley Hill Exit")
    CompleteZoneObby:InvokeServer()
end)

MiscTab:AddButton("Unlock all Characters", function()
    RequestUnlockCharacter:FireServer("sonic")
    task.wait(0.3)
    RequestTeleportToZone:InvokeServer("Lost Valley")
    RequestUnlockCharacter:FireServer("tails")
    task.wait(0.3)
    RequestTeleportToZone:InvokeServer("Emerald Hill")
    RequestUnlockCharacter:FireServer("knuckles")
end)

MiscTab:AddSwitch("Auto Combine Chaos", function(e)
getgenv().AutoCombine = e
while AutoCombine do task.wait(3)
local Data = require(game:GetService("ReplicatedFirst").Client.Controllers.Player.DataController)

for i, v in pairs(Data.Data.Pets) do
             Services.PetService.RE.CombinePet:FireServer(i, v)
	      end
    end
end)

MainTab:AddLabel("Collectables")

MainTab:AddButton("Collect all Wrenches", function()
for _,v in pairs(workspace:GetDescendants()) do
	if v.Name == "Card" and v:FindFirstChild("Attachment").Shine and v then
	       Services.CharacterShardService.RE.CharacterShardPickup:FireServer("mechanic tails", v.Parent.Name, true)
       end
    end
end)

MainTab:AddLabel("You must be on Chemical Plant. World 7")
MainTab:AddButton("Collect all Data Disks", function()
for _,v in pairs(workspace:GetDescendants()) do
if v.Name == "Card" and v:FindFirstChild("Sparkles") and v then
           Services.CharacterShardService.RE.CharacterShardPickup:FireServer("stealth suit sonic", v.Parent.Name)
       end 
    end
end)

SettingsTab:AddLabel("Settings")

local keybind = SettingsTab:AddKeybind("Toggle UI Keybind", function(obj)
	ui_options.toggle_key = obj
end, {
	["standard"] = Enum.KeyCode.RightShift,
})
