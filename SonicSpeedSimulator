for _,v in pairs(getconnections(game.Players.LocalPlayer.Idled)) do
    v:Disable()
end

loadstring(game:HttpGet("https://raw.githubusercontent.com/blooodlines/libs/main/elerium"))()

local qq = library:AddWindow("Sonic Speed Simulator")
local MainTab = qq:AddTab("Event")

local Knit = game:GetService("ReplicatedStorage").Knit
local Services = Knit.Services

MainTab:AddSwitch("Auto Collect Gifts", function(a)
getgenv().AutoCollect = a
while AutoCollect do task.wait(1)
	for _,v in pairs(workspace:FindFirstChild("World Currencies"):GetChildren()) do
     if not v:FindFirstChild("outer") and not v:FindFirstChild("ring") and v then
             Services.WorldCurrencyService.RE.PickupCurrency:FireServer(v.Name)
	      end
       end
    end
end)

MainTab:AddSwitch("Auto Collect Balloons & Crates", function(b)
getgenv().AutoCollect2 = b
while AutoCollect2 do task.wait(1)
for _,v in pairs(workspace.Map.Objects.Destructibles:GetChildren()) do
     if v:FindFirstChild("base").Attachment:FindFirstChild("dus") then
		     Services.DestructibleService.RE.ProcessDestructible:FireServer(v.Name) 
	      end
       end
    end
end)

MainTab:AddSwitch("Auto Collect Everything", function(c)
getgenv().AutoCollectt = c
while AutoCollectt do task.wait(1)
for _,v in pairs(workspace["World Currencies"]:GetChildren()) do
     if v.ClassName == "Model" then
		  Services.WorldCurrencyService.RE.PickupCurrency:FireServer(v.Name)
	      end
       end
    end
end)

MainTab:AddButton("Copy Discord Server", function()
	setclipboard("https://discord.gg/ssaVPA6atd")
end)

local keybind = MainTab:AddKeybind("Toggle UI", function(obj)
	ui_options.toggle_key = obj
end, {
	["standard"] = Enum.KeyCode.RightShift,
})
