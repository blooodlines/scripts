
loadstring(game:HttpGet("https://raw.githubusercontent.com/blooodlines/libs/main/elerium"))()

local qq = library:AddWindow("Sonic Speed Simulator")
local MainTab = qq:AddTab("Main")
local EventTab = qq:AddTab("Event")
local SettingsTab = qq:AddTab("Settings")

local LocalPlayer = game:GetService("Players").LocalPlayer

local WorldCurrencies = workspace:FindFirstChild("World Currencies")
local Knit = game:GetService("ReplicatedStorage").Knit
local Services = Knit.Services

local PickupCurrency = Services.WorldCurrencyService.RE.PickupCurrency
local ProcessDestructible = Services.DestructibleService.RE.ProcessDestructible

local RF = Services.ZoneService.RF
local RequestTeleportToZone = RF.RequestTeleportToZone
local CompleteZoneObby = RF.CompleteZoneObby
local RequestUnlockCharacter = Services.CharacterService.RE.RequestUnlockCharacter

EventTab:AddLabel("Firework Festival Event")

EventTab:AddSwitch("Auto Collect Fireworks", function(a)
getgenv().AutoCollect = a
while AutoCollect do task.wait(0.3)
for _,v in pairs(WorldCurrencies:GetChildren()) do
     if not v:FindFirstChild("outer") and not v:FindFirstChild("ring") and v then
               PickupCurrency:FireServer(v.Name)
	       end
       end
    end
end)

EventTab:AddSwitch("Auto Break Firework Crates", function(b)
getgenv().AutoCollect2 = b
while AutoCollect2 do task.wait(0.3)
for _,v in pairs(workspace.Map.Objects.Destructibles:GetChildren()) do
     if v:FindFirstChild("base").Attachment:FindFirstChild("dus") and v then
	          ProcessDestructible:FireServer(v.Name) 
	      end
       end
    end
end)

MainTab:AddSwitch("Auto Collect Everything", function(d)
getgenv().AutoCollect2 = d
while AutoCollect2 do task.wait(0.3)
for _,v in pairs(WorldCurrencies:GetChildren()) do
     if v:FindFirstChild("outer") and v:FindFirstChild("ring") and v then
               PickupCurrency:FireServer(v.Name)
	  end
       end
    end
end)

MainTab:AddSwitch("Auto Run", function(c)
getgenv().AutoRun = f
while AutoRun do task.wait()
game:GetService("ReplicatedStorage").Knit.Services.CharacterService.RE.UpdateCharacterState:FireServer(table.unpack({
    [1] = {
        ["Character"] = LocalPlayer.Character,
        ["CFrame"] = CFrame.new(-10490.124, 114, -6194.16895, 0.995638192, 0, -0.0932985097, 0, 1, 0, 0.0932985097, 0, 0.995638192),
        ["IsRunning"] = true,
    },
       }))
   end
end)


MainTab:AddButton("Unlock all Worlds", function()
    RequestTeleportToZone:InvokeServer("Lost Valley Obby", "Green Hill Exit")
    CompleteZoneObby:InvokeServer()
    task.wait(0.3)
    
    RequestTeleportToZone:InvokeServer("Emerald Hill Obby", "Lost Valley Exit")
    CompleteZoneObby:InvokeServer()
    task.wait(0.3)
    
    RequestTeleportToZone:InvokeServer("Snow Valley Obby", "Emerald Hill Exit")
    CompleteZoneObby:InvokeServer()

    task.wait(0.3)
    
    RequestTeleportToZone:InvokeServer("Hill Top Zone Obby", "Snow Valley Hill Exit")
    CompleteZoneObby:InvokeServer()
end)

MainTab:AddButton("Unlock all Characters", function()
    RequestUnlockCharacter:FireServer("sonic")

    RequestTeleportToZone:InvokeServer("Lost Valley Obby", "Green Hill Exit")
    CompleteZoneObby:InvokeServer()

    task.wait(0.3)

    RequestTeleportToZone:InvokeServer("Lost Valley")
    RequestUnlockCharacter:FireServer("tails")

    task.wait(0.3)

    RequestTeleportToZone:InvokeServer("Emerald Hill Obby", "Lost Valley Exit")
    CompleteZoneObby:InvokeServer()

    task.wait(0.3)

    RequestTeleportToZone:InvokeServer("Emerald Hill")
    RequestUnlockCharacter:FireServer("knuckles")
end)

MainTab:AddLabel("Only Unlocks Sonic, Tails and Knuckles")

EventTab:AddLabel("Stealth Suit Sonic")

EventTab:AddButton("Collect Data Disks", function()
for _,v in pairs(workspace:GetDescendants()) do
if v.Name == "Card" and v:FindFirstChild("Sparkles") and v then
           Services.CharacterShardService.RE.CharacterShardPickup:FireServer("stealth suit sonic", v.Parent.Name)
       end 
    end
end)

SettingsTab:AddLabel("Settings")

SettingsTab:AddButton("Copy Discord Server", function()
	setclipboard("https://discord.gg/ssaVPA6atd")
end)

local keybind = SettingsTab:AddKeybind("Toggle UI", function(obj)
	ui_options.toggle_key = obj
end, {
	["standard"] = Enum.KeyCode.RightShift,
})
